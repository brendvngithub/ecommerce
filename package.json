    "laravel-mix": "~4.0.15",
    "motion-ui": "^2.0.3",
    "slick-carousel": "^1.8.1",
    "vue": "^2.6.9",
    "what-input": "~4.1"
  },
  "devDependencies": {
    "resolve-url-loader": "^2.3.1",
